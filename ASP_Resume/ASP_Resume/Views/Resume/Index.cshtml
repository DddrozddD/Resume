                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <div class="title-block">
                                        

                                            <h2>@Model.FirstName @Model.SecondName</h2>
                                    <div class="owl-carousel text-rotation">
                                        @if (@Model.Resume.Specialities != null)
                                            {

                                                @foreach (var specialitie in Model.Resume.Specialities)
                                                {
                                                    <div class="item">
                                                    <div class="sp-subtitle">@specialitie.Name</div>
                                                    </div>
                                                }
                                            }
                                            </div>
                                        
                                    </div>
                                </div>
                            </div>

                        </div>
                    </section>
                    <!-- End of Home Subpage -->
                    <!-- About Me Subpage -->
                    <section data-id="about-me" class="animated-section">
                        <div class="page-title">
                            <h2>About <span>Me</span></h2>
                        </div>

                        <div class="section-content">
                            <!-- Personal Information -->
                            <div class="row">
                            <div class="col-xs-12 col-sm-7">
                                <p>@Model.Resume.About</p>
                            </div>

                                <div class="col-xs-12 col-sm-5">
                                    <div class="info-list">
                                        <ul>
                                            <li>
                                                <span class="title">Age</span>
                                                <span class="value">@Model.Age</span>
                                            </li>

                                            <li>
                                                <span class="title">Country</span>
                                                <span class="value">@Model.Country</span>
                                            </li>

                                            <li>
                                                <span class="title">Town</span>
                                                <span class="value">@Model.Town</span>
                                            </li>

                                            <li>
                                                <span class="title">e-mail</span>
                                                <span class="value">@Model.Email</span>
                                            </li>

                                            <li>
                                                <span class="title">Phone</span>
                                                <span class="value">@Model.Telephone</span>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <!-- End of Personal Information -->

                            <div class="white-space-50"></div>

                            <!-- Services -->
                            <div class="row">
                                <div class="col-xs-12 col-sm-12">
                                    <div class="block-title">
                                        <h3>What <span>I Do</span></h3>
                                    </div>
                                </div>
                            </div>

                            <div class="row">

                               


                                  

                                    
                                        @foreach (var skill in Model.Resume.Skills)
                                        {

                                            <div class="col-xs-12 col-sm-6">
                                                <div class="col-inner">
                                                    <div class="info-list-w-icon">
                                                        <div class="info-block-w-icon">
                                                            <div class="ci-text">
                                                                <h4>@skill.Name</h4>
                                                                <p>@skill.Description</p>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>


                                        }
                                    

                                


                            </div>
                            <!-- End of Services -->

                            <div class="white-space-30"></div>



                        </div>
                    </section>
                    <!-- End of About Me Subpage -->
                    <!-- Resume Subpage -->
                    <section data-id="resume" class="animated-section">
